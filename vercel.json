{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/templates/homepage"
    },
    {
      "src": "/login",
      "dest": "/api/templates/login"
    },
    {
      "src": "/usuarios",
      "dest": "/api/templates/usuarios"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)", 
      "destination": "/api/app"
    }
  ]
}


