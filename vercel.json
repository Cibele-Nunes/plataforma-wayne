{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/templates/homepage.html"
    },
    {
      "src": "/login",
      "dest": "/api/templates/login.html"
    },
    {
      "src": "/usuarios",
      "dest": "/api/templates/usuarios.html"
    }
  ]
}


